{
	"name": "symbiote/silverstripe-base",
	"description": "Ozzy: the Symbiote project installer",
	"require": {
		"php": ">=5.3.2",
		"gdmedia/ss-auto-git-ignore": "^0.0.3",
		"silverstripe/cms": "~3.5@stable",
		"silverstripe/framework": "~3.5@stable",
		"silverstripe-themes/simple": "*",
		"silverstripe/sqlite3": "1.4.x-dev",
		"symbiote/silverstripe-build": "~3.0",
		"symbiote/silverstripe-test-assist": "~3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"phpunit/php-code-coverage": "~1.2",
		"phpunit/phpunit-selenium": ">=1.2"
	},
	"extra": {
		"installer-name": "base",
		"branch-alias": {
			"dev-master": "4.0.x-dev"
		}
	},
	"replace": {
		"silverstripe-australia/base": "self.version"
	},
	"scripts": {
		"post-update-cmd": [
			"GDM\\SSAutoGitIgnore\\UpdateScript::Go"
		],
		"post-install-cmd": [
			"GDM\\SSAutoGitIgnore\\UpdateScript::Go"
		]
	},
	"minimum-stability": "dev"
}
